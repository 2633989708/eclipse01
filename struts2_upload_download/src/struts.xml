<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="struts3.1" extends="struts-default">
	<action name="register" class="com.action.Register">
		<result name="register">
			/showregister.jsp
		</result>
		<result name="input">
			/register.jsp
		</result>
	</action>
	<action name="register2" class="com.action.Register2">
		<result name="register2">
			/register2.jsp
		</result>
	</action>
	<action name="register3" class="com.action.Register3">
		<result name="register3">
			/register3.jsp
		</result>
	</action>
	
	<action name="download" class="com.action.DownloadAction"></action>
	<action name="ajax" class="com.action.AjaxAction"></action>
	<action name="listString" class="com.action.ListString"></action>
	<action name="listBean" class="com.action.ListBean"></action>
	<action name="mapString" class="com.action.MapString"></action>
	<action name="mapBean" class="com.action.MapBean"></action>
</package>
<constant name="struts.ui.theme" value="simple"></constant>
</struts>